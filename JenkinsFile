pipeline {
  agent any
    tools {nodejs "node"}
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/CHGovindaRao/Accessibility.git'
      }
    }
        
    stage('Install dependencies') {
      steps {
        sh 'npm config ls'
      }
    }
     
    stage('Test') {
      steps {
         sh 'node index.js'
      }
    }      
  }
}