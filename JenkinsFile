pipeline {
  agent any
    
  tools {nodejs "node"}
    

    stage('Cloning Git') {
      steps {
        
        url: 'git@github.com:CHGovindaRao/Accessibility.git'
      }
    }
        
        stage('Build') {
      steps {
          rtMaven.run pom: 'pom.xml', 
          goals: 'clean install'    
     
    stage('Test') {
      steps {
         sh 'node index.js'
      }
    }      
  }
}